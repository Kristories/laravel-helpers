{
    "name": "devtical/laravel-helpers",
    "description": "Helper Generator for Laravel",
    "keywords": [
        "laravel",
        "helper",
        "generator"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6|^7|^8|^9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "orchestra/testbench": "^6.24|^7.0",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Devtical\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devtical\\Helpers\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtical\\Helpers\\HelperServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist.php --allow-risky=yes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
