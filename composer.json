{
    "name": "wearedevtical/laravel-helpers",
    "description": "Helper Generator for Laravel",
    "license": "mit",
    "authors": [
        {
            "name": "Wahyu Kristianto",
            "email": "w.kristories@gmail.com",
            "homepage": "https://github.com/kristories"
        }
    ],
    "homepage": "https://github.com/wearedevtical/laravel-helpers",
    "keywords": [
        "laravel",
        "helper",
        "generator"
    ],
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^6.20.13|7.30.4|^8.22.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^4.9|^5.8|^6.3",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Devtical\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devtical\\Helpers\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtical\\Helpers\\HelperServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
