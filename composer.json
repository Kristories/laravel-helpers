{
    "name": "wearedevtical/laravel-helpers",
    "description": "Helper Generator for Laravel",
    "license": "mit",
    "authors": [
        {
            "name": "Wahyu Kristianto",
            "email": "w.kristories@gmail.com",
            "homepage": "https://github.com/kristories"
        }
    ],
    "homepage": "https://github.com/wearedevtical/laravel-helpers",
    "keywords": [
        "laravel",
        "helper",
        "generator"
    ],
    "type": "library",
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^4.0 || ^6.0 || ^7.0 || ^8.0",
        "phpunit/phpunit": "^8.3 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Devtical\\Helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devtical\\Helpers\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devtical\\Helpers\\HelperServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist.php --allow-risky=yes",
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
