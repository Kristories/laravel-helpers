{
  "name": "kristories/laravel-helpers",
  "description": "Helper Generator for Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "helper",
    "generator"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Wahyu Kristianto",
      "email": "w.kristories@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Kristories\\Helpers\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kristories\\Helpers\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kristories\\Helpers\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
